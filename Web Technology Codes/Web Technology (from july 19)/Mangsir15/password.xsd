<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.a.com" xmlns="http://www.a.com" elementFormDefault="qualified">
    <xs:element name="password">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:pattern value="[a-zA-Z0-9]{8}"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
</xs:schema>